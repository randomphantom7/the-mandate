    <script>
      
      $(document).ready(function(){
          $("#view_candidate").click(function(){
            axios.post('approval')
            .then(function (response) {
                 window.location.href = "approval";
            })
            .catch(function (error) {
                console.log(error);
            });
          });
        });

        $(document).ready(function(){
          $("#home").click(function(){
           axios.get('/teacher_login')
            .then(function (response) {
                window.location.href = "teacher_login";
            })
            .catch(function (error) {
                console.log(error);
            });
          });
        });


        $(document).ready(function(){
